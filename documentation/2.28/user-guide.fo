3     System.out.println("PUT CONTAINER " + container);
4 
5     URI uri = uriInfo.getAbsolutePath();
6     Container c = new Container(container, uri.toString());
7 
8     Response r;
9     if (!MemoryStore.MS.hasContainer(c)) {
