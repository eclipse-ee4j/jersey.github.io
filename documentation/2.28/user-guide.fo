7         defaultObjectMapper = createDefaultMapper();
8     }
9 
10     @Override
11     public ObjectMapper getContext(Class&lt;?&gt; type) {
12             return defaultObjectMapper;
13         }
14     }
15 
16     private static ObjectMapper createDefaultMapper() {
